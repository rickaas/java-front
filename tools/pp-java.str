    , "2"
    , "33"
    , "6"
    )
    ; s-step(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "28"
      , "4"
      , "33"
      , "6"
      )
    ; <(s-step(
        | "strategoxt/syntax/java-front/tools/pp-java.str"
        , "28"
        , "5"
        , "28"
        , "12"
        )
        ; tool-doc)> [ Usage("pp-java [OPTIONS]")
                     , Summary("Pretty-prints a Java abstract syntax tree in ATerm format to a Java source file.")
                     , OptionUsage()
                     , AutoReportBugs()
                     ]
    < s-exit(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "pp-java-usage"
      , "27"
      , "2"
      , "33"
      , "6"
      )
    + s-exit(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "pp-java-usage"
      , "27"
      , "2"
      , "33"
      , "6"
      )
      ; fail

  pp-java-about =
    s-enter(
    | "strategoxt/syntax/java-front/tools/pp-java.str"
    , "pp-java-about"
    , "34"
    , "2"
    , "43"
    , "6"
    )
    ; s-step(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "35"
      , "4"
      , "43"
      , "6"
      )
    ; <(s-step(
        | "strategoxt/syntax/java-front/tools/pp-java.str"
        , "35"
        , "5"
        , "35"
        , "12"
        )
        ; tool-doc)> [ AutoProgram()
                     , Author(Person("Martin Bravenboer", "martin.bravenboer@gmail.com"))
                     , GNU_LGPL("2002-2007", "Martin Bravenboer <martin.bravenboer@gmail.com>")
                     , Config([DefaultXTCRepository(), CurrentXTCRepository()])
                     ]
    < s-exit(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "pp-java-about"
      , "34"
      , "2"
      , "43"
      , "6"
      )
    + s-exit(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "pp-java-about"
      , "34"
      , "2"
      , "43"
      , "6"
      )
      ; fail