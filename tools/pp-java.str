module pp-java

imports
  org.strategoxt.imp.debuggers.stratego.runtime


imports
  libstratego-lib
  libjava-front
  libstratego-tool-doc


strategies
  main-pp-java =
    s-enter(
    | "strategoxt/syntax/java-front/tools/pp-java.str"
    , "main-pp-java"
    , "9"
    , "2"
    , "15"
    , "4"
    )
    ; s-step(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "10"
      , "4"
      , "15"
      , "4"
      )
    ; io-stream-wrap(
        s-step(
        | "strategoxt/syntax/java-front/tools/pp-java.str"
        , "11"
        , "6"
        , "11"
        , "9"
        )
        ; fail
      , s-step(
        | "strategoxt/syntax/java-front/tools/pp-java.str"
        , "12"
        , "6"
        , "12"
        , "18"
        )
        ; pp-java-usage
      , s-step(
        | "strategoxt/syntax/java-front/tools/pp-java.str"
        , "13"
        , "6"
        , "13"
        , "18"
        )
        ; pp-java-about
      , s-step(
        | "strategoxt/syntax/java-front/tools/pp-java.str"
        , "14"
        , "6"
        , "14"
        , "39"
        )
        ; (s-step(
           | "strategoxt/syntax/java-front/tools/pp-java.str"
           , "14"
           , "6"
           , "14"
           , "12"
           )
           ; pp-java
           <+ s-step(
              | "strategoxt/syntax/java-front/tools/pp-java.str"
              , "14"
              , "17"
              , "14"
              , "39"
              )
              ; report-failure-no-trace)
      )
    < s-exit(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "main-pp-java"
      , "9"
      , "2"
      , "15"
      , "4"
      )
    + s-exit(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "main-pp-java"
      , "9"
      , "2"
      , "15"
      , "4"
      )
      ; fail

  pp-java =
    s-enter(
    | "strategoxt/syntax/java-front/tools/pp-java.str"
    , "pp-java"
    , "17"
    , "2"
    , "20"
    , "25"
    )
    ; s-step(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "18"
      , "4"
      , "20"
      , "25"
      )
    ; (s-step(
       | "strategoxt/syntax/java-front/tools/pp-java.str"
       , "18"
       , "4"
       , "18"
       , "30"
       )
       ; ?( <s-step(
             | "strategoxt/syntax/java-front/tools/pp-java.str"
             , "18"
             , "7"
             , "18"
             , "22"
             )
             ; read-from-stream>
          , fout
          ))
    ; s-step(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "19"
      , "6"
      , "20"
      , "25"
      )
    ; (s-step(
       | "strategoxt/syntax/java-front/tools/pp-java.str"
       , "19"
       , "6"
       , "19"
       , "26"
       )
       ; pp-java-stream(|fout))
    ; s-step(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "20"
      , "6"
      , "20"
      , "25"
      )
    ; <(s-step(
        | "strategoxt/syntax/java-front/tools/pp-java.str"
        , "20"
        , "7"
        , "20"
        , "11"
        )
        ; fputs)> ("\n", fout)
    < s-exit(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "pp-java"
      , "17"
      , "2"
      , "20"
      , "25"
      )
    + s-exit(
      | "strategoxt/syntax/java-front/tools/pp-java.str"
      , "pp-java"
      , "17"
      , "2"
      , "20"
      , "25"
      )
      ; fail


